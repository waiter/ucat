.log-title {
  text-align: center;
  height: 134rpx;
  line-height: 50rpx;
  background-color: #ef5b30;
  color: #fff;
  padding: 0 0 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}

.log-title-item {
  display: flex;
  flex-direction: column;
  text-align: left;
}

.log-word-high {
  font-weight: bold;
  text-decoration: underline;
}

.log-list {
  display: flex;
  flex-direction: row;
  padding: 20rpx 0rpx;
  flex-wrap: wrap;
  justify-content: space-around;
}

.quan_view {
  margin: 10rpx 0;
  position:relative;
  font-size: 10px;
  background-color: #fff;
}

/*.quan_pic {
