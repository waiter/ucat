.quan_pic {
  width: 720rpx;
  height: 1022rpx;
}

.quan_btn_view {
  width: 100%;
  height: 60rpx;
  display: flex;
}

.quan_copy {
  font-size: 25rpx;
  border-radius: 0;
  flex-grow: 1;
}

.quan_copy_t {
  font-size: 25rpx;
  border-radius: 0;
  flex-grow: 1;
}

.quan_del {
  position: absolute;
  z-index: 10;
  right: 0;
  top: 10rpx;
}